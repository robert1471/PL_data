   "source": [
    "import pandas as pd"
   ],
   "metadata": {
    "collapsed": false,
    "pycharm": {
     "name": "#%%\n"
    }
   }
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "outputs": [],
   "source": [
    "writer = pd.ExcelWriter('all_gws.xlsx') # Arbitrary output name\n",
    "for i in range(1, 48):\n",
    "    df = pd.read_csv(f\"../Data/gw_{i}.csv\")\n",
    "    df.to_excel(writer,sheet_name=f\"gw_{i}\")\n",
    "\n",
    "writer.save()"
   ],
   "metadata": {
    "collapsed": false,
    "pycharm": {
     "name": "#%%\n"
    }
   }
  }
